service.yaml `
--git-branch=master `
 | kubectl apply -f -


fluxctl_linux_amd64 install `
--git-user=${gituser} `
--git-email=${gituser}@users.noreply.github.com `
--git-url=git@github.com:${gituser}/docker-development-youtube-series `
--git-path=../flux `
